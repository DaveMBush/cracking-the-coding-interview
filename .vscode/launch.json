  "version": "0.2.0",
  "configurations": [
{
  "name": "Current TS File",
  "type": "node",
  "request": "launch",
  "program": "${workspaceRoot}/node_modules/.bin/ts-node.cmd",
  "args": [
    "-r",
    "ts-node/register",
    "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js"
  ],
  "sourceMaps": true,
  "cwd": "${workspaceRoot}",
  "outFiles": [
    "/src/**/*.js",
    "/spec/**/*.spec.js"
  ],
  "protocol": "inspector",
}  ]
}
